<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
        http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
              version="2.2">
    <application>
        <el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
    </application>
    
	<converter>
	    <description>Conversor Objeto Persistente</description>
	    <converter-id>objPersistenteConverter</converter-id>
	    <converter-class>br.com.locadora.vmoura.dominio.controle.conversor.ObjetoPersistenteConversor</converter-class>
	</converter>
	<converter>
	    <description>Conversor Tipo Veículo</description>
	    <converter-id>tipoVeiculoConverter</converter-id>
	    <converter-class>br.com.locadora.vmoura.dominio.controle.conversor.TipoVeiculoConversor</converter-class>
	</converter>
	<converter>
	    <description>Conversor Tipo Item Adicional</description>
	    <converter-id>tipoItemAdicionalConverter</converter-id>
	    <converter-class>br.com.locadora.vmoura.dominio.controle.conversor.TipoItemAdicionalConversor</converter-class>
	</converter>
	<converter>
	    <description>Conversor Cliente</description>
	    <converter-id>clienteConverter</converter-id>
	    <converter-class>br.com.locadora.vmoura.dominio.controle.conversor.ClienteConversor</converter-class>
	</converter>
	<converter>
	    <description>Conversor Veículo</description>
	    <converter-id>veiculoConverter</converter-id>
	    <converter-class>br.com.locadora.vmoura.dominio.controle.conversor.VeiculoConversor</converter-class>
	</converter>
	<converter>
	    <description>Conversor Item Adicional</description>
	    <converter-id>itemAdicionalConverter</converter-id>
	    <converter-class>br.com.locadora.vmoura.dominio.controle.conversor.ItemAdicionalConversor</converter-class>
	</converter>
</faces-config>